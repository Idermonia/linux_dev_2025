#: guess.c:20
#, c-format
msgid "Think of a number between 1 and 100. Press Enter when ready.\n"
msgstr "Загадайте число от 1 до 100. Нажмите Enter когда будете готовы.\n"

#: guess.c:26
#, c-format
msgid "Is your number greater than %d? (Yes/No): "
msgstr "Ваше число больше чем %d? (Да/Нет): "

#: guess.c:30
#, c-format
msgid "Error: Input closed unexpectedly.\n"
msgstr "Ошибка: Ввод неожиданно прерван.\n"

#: guess.c:36
msgid "Yes"
msgstr "Да"

#: guess.c:38
msgid "No"
msgstr "Нет"

#: guess.c:41
#, c-format
msgid "Error: Please answer only 'Yes' or 'No'.\n"
msgstr "Ошибка: Пожалуйста, отвечайте только 'Да' или 'Нет'.\n"

#: guess.c:46 guess.c:51
#, c-format
msgid "Your number is %d!\n"
msgstr "Ваше число это %d!\n"
