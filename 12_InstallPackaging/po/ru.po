#: src/guess.c:90
msgid "Yes"
msgstr "Да"

#: src/guess.c:92
msgid "No"
msgstr "Нет"

#: src/guess.c:121
#, c-format
msgid "Usage: %s [-r]\n"
msgstr "Использование: %s [-r]\n"

#: src/guess.c:122
#, c-format
msgid "  -r    use Roman numerals\n"
msgstr "  -r    использовать римские цифры\n"

#: src/guess.c:127
#, c-format
msgid "Error: Unknown option '%s'\n"
msgstr "Ошибка: Неизвестная опция '%s'\n"

#: src/guess.c:128
#, c-format
msgid "Try '%s -h' for help\n"
msgstr "Введите '%s -h' для справки\n"

#: src/guess.c:139
#, c-format
msgid "Think of a Roman number between I and C. Press Enter when ready.\n"
msgstr "Загадайте римское число от I до C. Нажмите Enter, когда готовы.\n"

#: src/guess.c:141
#, c-format
msgid "Think of a number between 1 and 100. Press Enter when ready.\n"
msgstr "Загадайте число от 1 до 100. Нажмите Enter, когда готовы.\n"

#: src/guess.c:157
#, c-format
msgid "Is your number greater than %s? (Yes/No): "
msgstr "Ваше число больше, чем %s? (Да/Нет): "

#: src/guess.c:160
#, c-format
msgid "Is your number greater than %d? (Yes/No): "
msgstr "Ваше число больше, чем %d? (Да/Нет): "

#: src/guess.c:166
#, c-format
msgid "Error: Please answer only 'Yes' or 'No'.\n"
msgstr "Ошибка: Пожалуйста, отвечайте только 'Да' или 'Нет'.\n"

#: src/guess.c:175 src/guess.c:182
#, c-format
msgid "Your number is "
msgstr "Ваше число - "
